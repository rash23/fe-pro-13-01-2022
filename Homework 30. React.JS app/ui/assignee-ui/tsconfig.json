{
  "compilerOptions": {
		"outDir": "./dist",
    "target": "es5",
    "module": "es6",
		"moduleResolution": "node",
    "strict": true,
    "noFallthroughCasesInSwitch": true,
    "noImplicitReturns": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
		"jsx": "react",
		"allowSyntheticDefaultImports": true
  },
  "include": ["./src/**/*"],
  "exclude": ["node_modules", "dist", "public"]
}
